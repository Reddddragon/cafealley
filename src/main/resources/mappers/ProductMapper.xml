<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.cafealley.product.mapper.IProductMapper">

<insert id="">
	INSERT INTO product(prono, proname, proprice, prosellprice, procate, prodetail, prostock, filenum)
	VALUES(pro_seq.NEXTVAL, #{proname}, #{proprice}, #{prosellprice}, #{procate}, #{prodetail}, #{prostock}, #{filenum})
</insert>

<select id="select" resultType="com.spring.cafealley.command.ProductVO">
	SELECT * FROM product
	WHERE prono = #{prono}
</select>

<update id="">
</update>

<delete id="">
	DELETE FROM product
	WHERE prono = #{prono}
</delete>

</mapper>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  